<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
				   schema="dbo" 
				   assembly="manilab.Server" 
				   namespace="manilab.Server.Mappings" 
				   default-lazy="false"
				   default-cascade="none">
	<class name="manilab.Server.Entities.User" table="User">
		<id name="Id" column="UserId" type="Int32" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Descripcion"/>
        <property name="FechaCreacion" column="FechaCreacion" type="DateTime2"/>
        <property name="FechaActualizacion" column="FechaActualizacion" type="DateTime2"/>
	</class>
</hibernate-mapping>
